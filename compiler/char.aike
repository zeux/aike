let int(x: char): int =
    builtin(castNumber, x)

let char(x: int): char =
    builtin(castNumber, x)

let inrange(x, min, max) =
    int(x) >= int(min) and int(x) <= int(max)

let isdigit(ch) =
    inrange(ch, '0', '9')

let isalpha(ch) =
    inrange(ch, 'A', 'Z') or inrange(ch, 'a', 'z')

let isalnum(ch) =
    isalpha(ch) or isdigit(ch)

let isspace(ch) =
    ch == ' ' or ch == char(10)
