struct Point2
    x, y: int = 7

struct Point3
    x: int = 1
    y: int = 2
    z: int

var p0 = Point3 { x = 1, y = 2, z = 3 }
print(p0.x, p0.y, p0.z)
var p1 = Point3 { x = 4, z = 5 }
print(p1.x, p1.y, p1.z)
var p2 = Point3 { z = 8 }
print(p2.x, p2.y, p2.z)
var p3 = Point2 {}
print(p3.x, p3.y)

