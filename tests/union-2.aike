type test = One | Two | Basic int | Custom { x: int; y: int }

let x = One
let y = Two
let z = Basic(4)
let w = Custom(1, 2)

let process(x: test) = 
    match x with
    | One -> 10
    | Two -> 200
    | Basic x -> x
    | Custom x -> x.x + x.y

process(x) + process(y) + process(z) + process(w)

// OUTPUT:
// 217
