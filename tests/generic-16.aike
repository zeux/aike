type Lazy<'T> =
| Value 'T
| Cont Lazy<() -> 'T>

let value(x: Lazy<'T>) =
    match x with
    | Value v -> v
    | Cont c -> value(c)()

value(Cont(Cont(Value(fun -> (fun -> 2)))))

